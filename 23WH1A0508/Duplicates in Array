package Assignment;

import java.util.Scanner;

public class DuplicatesInArrays {

	public static void main(String[] args) {
		int array[]=new int[5];
		System.out.println("Enter elements in array");
		Scanner sc=new Scanner(System.in);
		for(int i=0;i<5;i++) {
			array[i]=sc.nextInt();
			
		}
		
		for(int j=0;j<5;j++) {
			for(int k=4;k>j;k--) {
				if (array[j]==array[k]) {
					System.out.println("The duplicate elememnts are "+array[j]);
					
				}
	
				
			}
		
		
		}
	}

}
