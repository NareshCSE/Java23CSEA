package Assignment;

public class NumberPattern {

	public static void main(String[] args) {
		int n = 5;



        // Loop through each row

        for (int i = 1; i <= n; i++) {

            // Print the decreasing part of the pattern

            for (int j = i; j >= 1; j--) {

                System.out.print(j);

            }

            // Print the increasing part of the pattern

            for (int j = 2; j <= i; j++) {

                System.out.print(j);

            }

            // Move to the next line after each row

            System.out.println();

        }

    }





	}








	
