package java553;
import java.util.Scanner;
public class PrimesInFibonacci {
	
	 public static boolean prime(int a) {
		int i = 1,cnt=0 ;
		while(i<a) {
			if(a % i == 0)
				cnt += 1;
			i++;
		}
		if(cnt==1)
			return true;
		else 
			return false;
		
	}
 
	public static void main(String[] args) {
		 Scanner sc = new Scanner(System.in);
		 int n,i,sum,a=0,b=1 ;
		 boolean primes;
		 System.out.println("enter length of fibonacci series");
		 n = sc.nextInt();
		 System.out.println("prime numbers in fibonacci series less than "+ n);
		 for(i=2;i<n;i++) {
			 sum = a+b;
			 a=b;
			 b=sum;
			 primes = prime(sum);
			 if(primes==true &&  sum<n)
				 System.out.println(sum);  
		 }
	}
}
 
